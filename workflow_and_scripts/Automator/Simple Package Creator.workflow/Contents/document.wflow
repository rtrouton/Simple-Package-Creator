					<string>on run {input, parameters}		-- this repeat loop prevents empty strings from being submitted for the package name value	set q to 0	repeat while q is 0		set result to text returned of (display dialog "Enter a Name For Your Installer Package:" default answer "My Great Application Installer Package")		if result is not "" then			set pkgname to result			set q to 1		end if	end repeat	-- this repeat loop prevents empty strings from being submitted for the package identifier value	set q to 0	repeat while q is 0		set result to text returned of (display dialog "Enter a Package Identifier:" default answer "com.github.simple_package")		if result is not "" then			set pkgid to result			set q to 1		end if	end repeat	-- this repeat loop prevents empty strings from being submitted for the version identifier value	set q to 0	repeat while q is 0		set result to text returned of (display dialog "Enter a Version Identifier:" default answer "1.0")		if result is not "" then			set pkgvers to result			set q to 1		end if	end repeat			-- Remove any existing build directories that have the same name as the new installer package		do shell script "/bin/rm -rf /tmp/" &amp; quoted form of pkgname &amp; "/" with administrator privileges		-- Create the build directories for the installer package		do shell script "/bin/mkdir /tmp/" &amp; quoted form of pkgname &amp; "" with administrator privileges	do shell script "/bin/mkdir /tmp/" &amp; quoted form of pkgname &amp; "/Payload" with administrator privileges		-- Unzip the application into the correct build directory and remove the __MACOSX directory, 	-- which is created as part of the uncompression process from the destination directory		do shell script "/usr/bin/unzip -d /tmp/" &amp; quoted form of pkgname &amp; "/Payload /tmp/simple_package_creator.zip" with administrator privileges	do shell script "/bin/rm -rf /tmp/" &amp; quoted form of pkgname &amp; "/Payload/__MACOSX" with administrator privileges		-- Build component plist file		do shell script "/usr/bin/pkgbuild --analyze --root /tmp/" &amp; quoted form of pkgname &amp; "/Payload /tmp/" &amp; quoted form of pkgname &amp; ".plist"		-- Edit component plist file to set the BundleIsRelocatable value to false	-- This will prevent the installer process from installing the application	-- anywhere other than /Applications		do shell script "/usr/libexec/plistbuddy -c 'Set :0:BundleIsRelocatable boolean false' /tmp/" &amp; quoted form of pkgname &amp; ".plist"		-- Build the installer package		do shell script "/usr/bin/pkgbuild --identifier " &amp; quoted form of pkgid &amp; " --version " &amp; quoted form of pkgvers &amp; " --install-location //Applications --component-plist /tmp/" &amp; quoted form of pkgname &amp; ".plist --root /tmp/" &amp; quoted form of pkgname &amp; "/Payload /tmp/" &amp; quoted form of pkgname &amp; "/" &amp; quoted form of pkgname &amp; ".pkg" with administrator privileges		-- remove zipped copy of application		do shell script "/bin/rm -rf /tmp/simple_package_creator.zip"		-- remove component plist file		do shell script "/bin/rm /tmp/" &amp; quoted form of pkgname &amp; ".plist"		-- Display dialog that the payload-free package has been created		display alert ((pkgname) as string) &amp; ".pkg has been created."		-- Open a new Finder window that shows the new package		do shell script "/usr/bin/open /tmp/" &amp; quoted form of pkgname &amp; ""		return inputend run</string>
